function [H] = train_lr()

    NUM_FEATURES = 513;

    % Get train and validation sets.
    [C, N] = formatData();
    
    % Create Parameter matrix.
    W = rand(10, NUM_FEATURES);
    
    T = C{1};
    for i = 2:10
        T = vertcat(T, C{2});
    end
    [N,D] = size(T);
    Y = T(:,1);
    X = T(:,2:D);
    
    
    % Build hypothesis matrix.
    H = hypothesis_lr(X, Y, W);
    
    % Perform gradient decent.
    



    function c = cost( H )
       
        for i = 1:10
            
            h = H{i};
            
            
            
        end
        
        arg1 = (-y) * log(h);
        arg2 = (1 - y) * log(1 - h);
        c = arg1 - arg2;
    end
    
    


end



