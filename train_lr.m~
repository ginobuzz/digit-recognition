function [X] = train_lr()

    % Get train and validation sets.
    [T,V] = formatData();
    
    % Apply sigmoid function to each matrix element.
    X = T{1};
    [M,N] = size(X);
    for i = 1:M
        for j = 1:N
            X(i,j) = sigmoid(X(i,j));
        end
    end
    
    W = (X'*X)
    
    function [phi] = sigmoid(x)
        phi = 1 / (1 + exp(-x));
    end

end